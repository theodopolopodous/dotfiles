[user]
	name = Jason
	email = jason@local
[core]
	editor = vim
	excludesfile = ~/.gitignore
	autocrlf = input
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[web]
	browser = safari
[credential]
	helper = osxkeychain
[push]
	default = simple
[merge]
	tool = kdiff3
[difftool]
	prompt = false
[alias]
	c = commit
	co = checkout
	ca = commit -a
	cm = commit -m
	cam = commit -am
	d = diff
	dc = diff --cached
	l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit
